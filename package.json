{
  "name": "firebase_react_testing_sample",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "test": "vitest --mode testing",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "deploy": "npm run build && firebase deploy --except functions",
    "emulator:serve": "firebase emulators:start --import=data/firebase --export-on-exit=data/firebase",
    "emulator:testing": "cd tests/emulator && firebase emulators:start"
  },
  "dependencies": {
    "firebase": "^9.15.0",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-firebase-hooks": "^5.1.1",
    "vite-tsconfig-paths": "^4.0.3"
  },
  "devDependencies": {
    "@firebase/rules-unit-testing": "^2.0.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/lodash-es": "^4.17.6",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "@vitejs/plugin-react": "^2.1.0",
    "eslint": "^8.30.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^24.0.0",
    "eslint-plugin-import": "^2.24.1",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fishery": "^2.2.2",
    "happy-dom": "^8.1.1",
    "prettier": "^2.8.1",
    "typescript": "^4.9.4",
    "vite": "^3.1.0",
    "vitest": "^0.26.2"
  }
}
